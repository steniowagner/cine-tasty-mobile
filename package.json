{
  "name": "cine-tasty-mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build-android": "cd android && ./gradlew assembleRelease && cd ..",
    "android-release": "npx react-native run-android --variant=release",
    "start:clean": "npx react-native start --reset-cache",
    "android": "npx react-native run-android",
    "lint:fix": "eslint --fix --ext .js,.ts,.tsx .",
    "prettier:fix": "prettier '**/*.+(ts|js|tsx)' --write",
    "start": "npx react-native start",
    "ios": "npx react-native run-ios",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "clear:ios": "cd ios && rm -rf build && cd ..",
    "clear:android": "cd android && ./gradlew clean && cd ..",
    "clear:cache": "watchman watch-del-all",
    "clear": "kill $(lsof -t -i:8081); yarn clear:ios; yarn clear:android; yarn clear:cache",
    "schema:download": "npx apollo client:download-schema --endpoint=http://localhost:3000",
    "schema:generate": "npx apollo client:codegen src/types/schema.ts --localSchemaFile=schema.json --includes=src/**/*.ts,src/**/*.tsx --addTypename --target=typescript --outputFlat",
    "generate:types": "yarn schema:download && yarn schema:generate"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(ts|tsx)": [
      "yarn prettier:fix",
      "yarn lint:fix"
    ]
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@react-native-community/async-storage": "^1.8.0",
    "@react-native-community/masked-view": "^0.1.6",
    "@react-native-community/slider": "^2.0.9",
    "@react-navigation/bottom-tabs": "^5.9.2",
    "@react-navigation/material-top-tabs": "^5.2.19",
    "@react-navigation/native": "^5.7.6",
    "@react-navigation/stack": "^5.9.3",
    "add": "^2.0.6",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "i18next": "^19.3.2",
    "react": "16.13.1",
    "react-i18next": "^11.3.3",
    "react-native": "0.63.4",
    "react-native-dotenv": "^2.5.1",
    "react-native-fast-image": "^8.3.4",
    "react-native-gesture-handler": "^1.6.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-navigation-bar-color": "^2.0.1",
    "react-native-reanimated": "^1.7.0",
    "react-native-restart": "^0.0.17",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-screens": "^2.2.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-status-bar-height": "^2.5.0",
    "react-native-svg": "^12.0.3",
    "react-native-tab-view": "^2.15.2",
    "react-native-vector-icons": "^7.1.0",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@graphql-codegen/cli": "^1.15.4",
    "@graphql-codegen/fragment-matcher": "^1.15.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@testing-library/react-hooks": "^3.2.1",
    "@testing-library/react-native": "^7.0.2",
    "@types/jest": "^26.0.20",
    "@types/react-native": "^0.63.2",
    "@types/react-test-renderer": "^16.9.2",
    "@types/styled-components": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "apollo-link-schema": "^1.2.4",
    "babel-jest": "^25.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.5.0",
    "graphql-tools": "^4.0.7",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "metro-react-native-babel-preset": "^0.59.0",
    "mockdate": "^2.0.5",
    "prettier": "^1.19.1",
    "react-test-renderer": "16.13.1",
    "ts-jest": "^26.5.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
