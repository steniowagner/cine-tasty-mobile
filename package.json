{
  "name": "CineTasty",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "android:run": "react-native run-android",
    "android:clear": "cd android && ./gradlew clean && cd ..",
    "ios:run": "react-native run-ios",
    "ios:clear": "cd ios && rm -rf build && cd ..",
    "lint": "eslint .",
    "lint:fix": "eslint --fix --ext .ts,.tsx .",
    "prettier:fix": "prettier '**/*.+(ts|tsx)' --write",
    "clear:cache": "watchman watch-del-all",
    "clear": "kill $(lsof -t -i:8081); yarn ios:clear; yarn android:clear; yarn clear:cache",
    "test": "jest --passWithNoTests --no-cache",
    "test:integration": "npm test -- --watch --testMatch **/*.spec.tsx --runInBand",
    "test:unit": "npm test -- --watch --testMatch **/*.test.tsx **/*.test.ts",
    "test:coverage": "jest --coverage --passWithNoTests",
    "prepare": "husky install"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/jest": "^29.5.5",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "ts-jest": "^29.1.1",
    "typescript": "4.8.4",
    "husky": "^8.0.0"
  },
  "engines": {
    "node": ">=16"
  }
}
